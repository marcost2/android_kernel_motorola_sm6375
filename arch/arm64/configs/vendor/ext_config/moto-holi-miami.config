CONFIG_LEDS_LM3697_MMI=m
CONFIG_LEDS_AW2033=m
# CONFIG_TOUCHSCREEN_NT36XXX is not set
CONFIG_TOUCHSCREEN_GOODIX_BRL=m
CONFIG_TOUCHSCREEN_GOODIX_BRL_SPI=y
