CONFIG_INPUT_TOUCHSCREEN_MMI=m
# CONFIG_TOUCHCLASS_MMI_DEBUG_INFO is not set
# CONFIG_TOUCHCLASS_MMI_GESTURE_POISON_EVENT is not set
CONFIG_TOUCHCLASS_MMI_MULTIWAY_UPDATE_FW=y
CONFIG_DRM_PANEL_NOTIFICATIONS=y
# CONFIG_DRM_PANEL_EVENT_NOTIFICATIONS is not set
CONFIG_DRM_DYNAMIC_REFRESH_RATE=y
# CONFIG_BOARD_USES_DOUBLE_TAP_CTRL is not set
# CONFIG_GTP_DDA_STYLUS is not set
CONFIG_AWINIC_HAPTIC=y
# CONFIG_AW8695_HAPTIC is not set
CONFIG_AW862X_HAPTIC=m
CONFIG_MMI_SYS_TEMP=m
CONFIG_SARSENSOR_SX937X=m
