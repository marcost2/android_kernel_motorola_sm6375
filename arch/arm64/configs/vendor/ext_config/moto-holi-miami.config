# CONFIG_REGULATOR_QPNP_LCDB is not set
CONFIG_AW882XX_STEREO_SMARTPA=y
CONFIG_AW882XX_PARAMS_FROM_BIN=y
CONFIG_AW882XX_MAPSIZE_16K=y
# CONFIG_UFSTW is not set
CONFIG_QUP0_SE2_AP=y
# CONFIG_CAMERA_FLASH_PWM is not set
CONFIG_CRYPTO_CHACHA20POLY1305=y
CONFIG_CAM_SENSOR_PROBE_RETRY=y
CONFIG_CAMERA_CCI_ADDR_SWITCH=y
CONFIG_IS_XFER_IN_PROGRESS=y
CONFIG_MMAP_READAROUND_LIMIT=y
CONFIG_MMI_ANNOTATE=m
CONFIG_MMI_INFO=m
CONFIG_MOTO_F_USBNET=m
CONFIG_MMI_RELAY=m
CONFIG_SENSORS_CLASS=m
CONFIG_LEDS_AW99703=m
